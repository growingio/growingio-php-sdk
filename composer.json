{
  "name": "growingio/php-sdk",
  "type": "library",
  "homepage": "http://www.growingio.com",
  "description": "PHP SDK for GrowingIO",
  "keywords": ["sdk", "growingio"],
  "license": "MIT",
  "require": {
    "php": ">=5.5.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "3.*",
    "phpunit/phpunit": "5.3.*"
  },
  "autoload": {
    "psr-4": {
      "com\\growingio\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "com\\growingio\\test\\": "test/"
    }
  },
  "scripts": {
    "check": "./vendor/bin/phpcs",
    "autofix": "./vendor/bin/phpcbf",
    "test": "./vendor/bin/phpunit --no-coverage",
    "coverage": "./vendor/bin/phpunit"
  },
  "minimum-stability": "stable"
}